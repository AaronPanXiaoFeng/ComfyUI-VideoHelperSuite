{
    "main_pass":
    [
        "-n", "-c:v", "libx265",
        "-vtag", "hvc1",
        "-pix_fmt", ["pix_fmt", ["yuv420p10le", "yuv420p"]],
        "-crf", ["crf","INT", {"default": 22, "min": 0, "max": 100, "step": 1}],
        "-preset", "medium",
        "-vf", "scale=out_color_matrix=bt709",
        "-x265-params", "log-level=quiet"
    ],
    "fake_trc": "bt709",
    "audio_pass": ["-c:a", "aac"],
    "save_metadata": ["save_metadata", "BOOLEAN", {"default": true}],
    "extension": "mp4"
}
